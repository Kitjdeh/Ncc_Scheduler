<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/setting_nav"
    app:startDestination="@id/mainActivity">

    <activity
        android:id="@+id/mainActivity"
        android:name="com.ncc.presentation.view.main.MainActivity"
        android:label="MainActivity">
        <action
            android:id="@+id/action_mainActivity_to_settingActivity"
            app:destination="@id/settingActivity" />

    </activity>
    <activity
        android:id="@+id/settingActivity"
        android:name="com.ncc.presentation.view.main.setting.SettingActivity"
        android:label="SettingActivity">
        <action
            android:id="@+id/action_settingActivity_to_mainActivity"
            app:destination="@id/mainActivity" />

    </activity>
</navigation>